{
  "name": "Business Chat",
  "version": "0.0.7",
  "author": "Ionic Framework",
  "homepage": "https://ionicframework.com/",
  "main": "electron/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "desktop": "electron electron/main.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dist": "electron-builder",
    "lint": "ng lint"
