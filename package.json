      "electron/**/*",
      "www/**/*",
      "package.json",
      "!.angular/**",
      "!.git/**"
    ],
    "directories": {
      "output": "dist-desktop"
    },
    "npmRebuild": false,
    "publish": [
      {
        "provider": "github",
        "owner": "ReyeMx96",
        "repo": "businesschat",
        "private": false
      }
    ]
  },
  "private": true,
